{
  "name": "share_nb",
  "version": "0.2.0",
  "description": "Jupyter Notebook submission to GCP",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/gclouduniverse/nova-jupyterlab-plugin",
  "bugs": {
    "url": "https://github.com/gclouduniverse/nova-jupyterlab-plugin/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Abdalla Abdulmonem, Viacheslav Kovalevskyi",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/*.css",
    "style/images/*.png"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/gclouduniverse/nova-jupyterlab-plugin.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "prepare": "npm run clean && npm run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^1.0.2",
    "@jupyterlab/apputils": "^1.0.2",
    "@jupyterlab/coreutils": "^3.0.0",
    "@jupyterlab/docregistry": "^1.0.2",
    "@jupyterlab/filebrowser": "^1.0.2",
    "@jupyterlab/mainmenu": "^0.8.0",
    "@jupyterlab/notebook": "^1.0.2",
    "@phosphor/disposable": "^1.1.2",
    "@phosphor/widgets": "^1.6.0",
    "file-system": "^2.2.2",
    "fs-extra": "^7.0.1",
    "typestyle": "^2.0.0",
    "lodash.mergewith": "^4.6.2"
  },
  "devDependencies": {
    "@types/fs-extra": "~4.0.3",
    "rimraf": "^2.6.1",
    "typescript": "~3.1.1",
    "typestyle": "^2.0.0"
  },
  "jupyterlab": {
    "extension": true,
    "discovery": {
      "server": {
        "managers": [
          "pip"
        ],
        "base": {
          "name": "share_nb"
        }
      }
    }
  }
}
